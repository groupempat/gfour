<%- include('./partials/header.ejs') %>
<br />
<form action="/books/findByUserID/?" method="GET" style="padding-bottom: 25px;">
  <input type="text" name="UserID" id="UserID" placeholder="Find Book" />
</form>
<table id="example" class="table table-striped table-bordered mt-1" style="width: 100%;">
  <thead class="thead-dark">
    <tr>
      <th>No</th>
      <th>title</th>
      <th>price</th>
      <th>quantity</th>
      <th>Author</th>

    </tr>
  </thead>
  <tbody>
    <% for(let i= 0; i < results.length; i++) { %>
    <tr>
      <td><%= i+1; %></td>
      <td><%= results[i].title %></td>
      <td><%= results[i].price %></td>
      <td><%= results[i].quantity %></td>
      <td><%= results[i].Author %></td>
    </tr>
    <% } %>
  </tbody>
</table>